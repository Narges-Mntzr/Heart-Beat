#: heartbeat/services.py:13
msgid "Just now"
msgstr "لحظاتی پیش"

#: heartbeat/services.py:16
#, python-brace-format
msgid "{} mins ago"
msgstr "{} دقیقه پیش"

#: heartbeat/services.py:19
#, python-brace-format
msgid "{hours} hours ago"
msgstr "{hours} ساعت پیش"

#: heartbeat/services.py:22
#, python-brace-format
msgid "{days} days ago"
msgstr "{days} روز پیش"

#: heartbeat/views.py:26
msgid "Username and password are required."
msgstr "وارد کردن نام کاربری و رمز عبور الزامی است."

#: heartbeat/views.py:30
msgid "Username must be at least 6 characters long."
msgstr "نام کاربری باید حداقل ۶ کاراکتر داشته باشد."

#: heartbeat/views.py:35
msgid "Username contains invalid characters."
msgstr "نام کاربری شامل کاراکترهای غیرمجاز است."

#: heartbeat/views.py:39
msgid "Username already exists."
msgstr "این نام کاربری قبلاً ثبت شده است."

#: heartbeat/views.py:53
msgid "Invalid password."
msgstr "رمز عبور نادرست است."

#: heartbeat/views.py:56
msgid "User not found."
msgstr "کاربری با این مشخصات یافت نشد."

#: heartbeat/views.py:64
msgid "Username is necessary."
msgstr "وارد کردن نام کاربری الزامی است."

#: heartbeat/views.py:73
msgid "You are already following this user."
msgstr "شما قبلاً این کاربر را دنبال کرده‌اید."

#: heartbeat/views.py:81 heartbeat/views.py:172
msgid "User not found"
msgstr "کاربر یافت نشد."

#: heartbeat/views.py:142
msgid "heartbeat recorded"
msgstr "ضربان قلب ثبت شد."

#: heartbeat/views.py:195
msgid "Missing fields."
msgstr "برخی فیلدها پر نشده‌اند."

#: heartbeat/views.py:202
msgid "You can only send messages to your followers."
msgstr "تنها می‌توانید به دنبال‌کنندگان خود پیام ارسال کنید."

#: heartbeat/views.py:207
msgid "The message text must be a maximum of 100 characters."
msgstr "متن پیام نباید بیش از ۱۰۰ نویسه باشد."

#: heartbeat/views.py:212
msgid "Message sent"
msgstr "پیام با موفقیت ارسال شد."

#: heartbeat/views.py:232
msgid "Message not found"
msgstr "پیام مورد نظر یافت نشد."
